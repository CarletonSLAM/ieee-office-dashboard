{
  "name": "ieee-office-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "cross-fetch": "^1.1.1",
    "koa": "^2.4.1",
    "koa-router": "^7.3.0",
    "koa-static": "^4.0.2",
    "material-ui": "1.0.0-beta.25",
    "moment": "^2.20.1",
    "node-rsa": "^0.4.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.17",
    "react-slick": "^0.16.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "typeface-roboto": "0.0.45"
  },
  "scripts": {
    "client-dev": "react-scripts start",
    "start": "concurrently 'npm run server' 'npm run client-dev'",
    "build": "node ./config/build.js && react-scripts build && npm run delete-map-files",
    "delete-map-files": "find ./build -name '*.map' -delete",
    "lint": "eslint ./src --fix",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server": "node server/index.js",
    "serve": "npm run build && npm run server"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-react": "^7.7.0"
  }
}
