{
    "parser": "babel-eslint",
    "extends": [ "plugin:react/recommended" ],
    "plugins": [
        "react"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
        "indent": ["error", 4],
        "comma-dangle": ["error", "never"],
        "max-len": [1,200],
        "semi": [2, "never"],
        "no-trailing-spaces": "error",
        "no-param-reassign": 0
    }
}
