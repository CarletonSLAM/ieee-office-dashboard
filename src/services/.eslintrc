{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": ["error", 4],
        "comma-dangle": ["error", "never"],
        "max-len": [1,200],
        "semi": ["error", "never"],
        "no-trailing-spaces": "error",
        "consistent-return": 0,
        "prefer-promise-reject-errors": 0,
        "import/prefer-default-export": 0
    }
}
